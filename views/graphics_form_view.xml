			            <option value="1">5" x 7"</option>
			            <option value="2">6" x 6"</option>
			            <option value="3">5.5" x 8.5"</option>
			            <option value="4">8" x 8"</option>
			            <option value="5">8.5" x 11"</option>
			            <option value="6">8.5" x 14"</option>
